<% include blocks/studheader.ejs %>
		
		<!-- Contact -->
		<section id="contact-us">
			<div class="container">
				<div class="row">
					<div class="col-sm-12">
						<div class="title">
							<h3>Student <span><%=name%></span></h3>
						</div>
					</div>
				</div>
			</div>
		
		
		
			<div class="contact">
				<div class="container">
					<table class="table table-hover" >
					  <tr>
					    <th>Name</th>
					    <th>Email</th>
					    <th>Phone</th>
					  </tr>

					  <% data.forEach(function(obj){%>
					    <tr>
					    <td> <%=obj.name%> </td>
					    <td> <%=obj.mail%> </td>
					    <td> <%=obj.phone%> </td>
					    </tr>
					  <% })%>
					  
					</table>
				</div>
			</div>
			
		</section>		
		<!-- Contact -->
		
